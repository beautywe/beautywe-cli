    "inquirer": "^6.2.1",
    "lodash": "^4.17.11",
    "signale": "^1.3.0",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "commitizen": "^3.1.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "validate-commit-msg": "^2.14.0"
